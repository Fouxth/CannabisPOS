{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/socket.io/:path*",
            "destination": "https://acrobat-screen-trace-referenced.trycloudflare.com/socket.io/:path*"
        },
        {
            "source": "/api/(.*)",
            "destination": "https://acrobat-screen-trace-referenced.trycloudflare.com/api/$1"
        },
        {
            "source": "/((?!api|socket.io|assets|favicon.ico).*)",
            "destination": "/index.html"
        }
    ]
}