{
    "name": "cannabispos-server",
    "version": "1.0.0",
    "description": "Backend API for CannabisPOS",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:seed": "tsx prisma/seed.ts",
        "db:studio": "prisma studio",
        "db:management:generate": "prisma generate --schema=./prisma/management/schema.prisma",
        "db:management:migrate": "prisma migrate dev --schema=./prisma/management/schema.prisma --name init_management",
        "db:management:studio": "prisma studio --schema=./prisma/management/schema.prisma"
    },
    "keywords": [
        "pos",
        "cannabis",
        "api"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^6.19.0",
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^8.2.1",
        "jsonwebtoken": "^9.0.3",
        "socket.io": "^4.8.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^22.16.5",
        "@types/socket.io": "^3.0.1",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "prisma": "^6.1.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "tsx": "^4.19.2",
        "typescript": "^5.8.3"
    }
}
